        <errorMessage>Recurring Payment Schedule Date must have a value between 0 and 31 as the Recurring Payment Schedule Type is &apos;Monthly&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IATSPayment__Recurring_Payment_Schedule_Date_Not_Appl</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
IATSPayment__Complete_Batch_Update__c,

OR(
AND(
ISPICKVAL(IATSPayment__IATS_Account__r.IATSPayment__Direct_Debit_Processor__c, &apos;Rapidata&apos;),
IATSPayment__Method_of_Payment__c = &apos;ACH&apos; || IATSPayment__Method_of_Payment__c = &apos;Direct Debit&apos;
),
LEN(IATSPayment__IATS_Account__r.IATSPayment__Password__c) = 36
),

!ISBLANK(IATSPayment__Recurring_Payment_Schedule_Date__c)
)</errorConditionFormula>
        <errorDisplayField>IATSPayment__Recurring_Payment_Schedule_Date__c</errorDisplayField>
        <errorMessage>Recurring Payment Schedule Date is not applicable for the iATS Merchant Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IATSPayment__Recurring_Payment_Schedule_Date_Weekly</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
IATSPayment__Complete_Batch_Update__c,
IATSPayment__Recurring_Payment_Schedule_Type__c = &apos;Weekly&apos;,
OR(
IATSPayment__Recurring_Payment_Schedule_Date__c &lt;=0,
IATSPayment__Recurring_Payment_Schedule_Date__c &gt; 7
)
)</errorConditionFormula>
        <errorDisplayField>IATSPayment__Recurring_Payment_Schedule_Date__c</errorDisplayField>
        <errorMessage>Recurring Payment Schedule Date must have a value between 0 and 7 as the Recurring Payment Schedule Type is &apos;Monthly&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IATSPayment__Recurring_Payment_Schedule_Type</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
IATSPayment__Complete_Batch_Update__c,
OR(
!ISBLANK(IATSPayment__Recurring_Payment_Amount__c), 
!ISBLANK(IATSPayment__Recurring_Payment_Status__c),
!ISBLANK(IATSPayment__Recurring_Payment_End_Date__c),
!ISBLANK(IATSPayment__Recurring_Payment_Schedule_Date__c),
!ISBLANK(IATSPayment__Recurring_Payment_Begin_Date__c)
),
ISBLANK(IATSPayment__Recurring_Payment_Schedule_Type__c)
)</errorConditionFormula>
        <errorDisplayField>IATSPayment__Recurring_Payment_Schedule_Type__c</errorDisplayField>
        <errorMessage>Recurring Payment Schedule Type must have a value if the other Recurring Payment fields are populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IATSPayment__Recurring_Payment_Schedule_Type_Value</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
IATSPayment__Complete_Batch_Update__c,
!ISBLANK(IATSPayment__Recurring_Payment_Schedule_Type__c),
IATSPayment__Recurring_Payment_Schedule_Type__c != &apos;Weekly&apos;,
IATSPayment__Recurring_Payment_Schedule_Type__c != &apos;Monthly&apos;,
IATSPayment__Recurring_Payment_Schedule_Type__c != &apos;Quarterly&apos;,
IATSPayment__Recurring_Payment_Schedule_Type__c != &apos;Annually&apos;
)</errorConditionFormula>
        <errorDisplayField>IATSPayment__Recurring_Payment_Schedule_Type__c</errorDisplayField>
        <errorMessage>Recurring Payment Schedule Type must be &apos;Weekly&apos;, &apos;Monthly&apos;, &apos;Quarterly&apos;, or &apos;Annually&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IATSPayment__Recurring_Payment_Schedule_Type_Weekly</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
IATSPayment__Complete_Batch_Update__c == true, 
IATSPayment__Recurring_Payment_Schedule_Type__c = &apos;Weekly&apos;,
ISPICKVAL(IATSPayment__IATS_Account__r.IATSPayment__Direct_Debit_Processor__c, &apos;Rapidata&apos;)
)</errorConditionFormula>
        <errorDisplayField>IATSPayment__Recurring_Payment_Schedule_Type__c</errorDisplayField>
        <errorMessage>Weekly is not available for Rapidata UK Direct Debit merchant accounts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IATSPayment__Recurring_Payment_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
IATSPayment__Complete_Batch_Update__c,
OR(
!ISBLANK(IATSPayment__Recurring_Payment_Begin_Date__c),
!ISBLANK(IATSPayment__Recurring_Payment_Amount__c), 
!ISBLANK(IATSPayment__Recurring_Payment_End_Date__c),
!ISBLANK(IATSPayment__Recurring_Payment_Schedule_Date__c),
!ISBLANK(IATSPayment__Recurring_Payment_Schedule_Type__c)
),

ISBLANK(IATSPayment__Recurring_Payment_Status__c)
)</errorConditionFormula>
        <errorDisplayField>IATSPayment__Recurring_Payment_Status__c</errorDisplayField>
        <errorMessage>Recurring Payment Status must have a value if the other Recurring Payment fields are populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IATSPayment__Recurring_Payment_Status_Value</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
IATSPayment__Complete_Batch_Update__c,
!ISBLANK(IATSPayment__Recurring_Payment_Status__c),
IATSPayment__Recurring_Payment_Status__c != &apos;Active&apos;,
IATSPayment__Recurring_Payment_Status__c != &apos;Inactive&apos;
)</errorConditionFormula>
        <errorDisplayField>IATSPayment__Recurring_Payment_Status__c</errorDisplayField>
        <errorMessage>If the Recurring Payment Status is provided it must be set to &apos;Active&apos; or &apos;Inactive&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IATSPayment__iATS_Merchant_Account</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
IATSPayment__Complete_Batch_Update__c == true, 
ISCHANGED(IATSPayment__IATS_Account__c),
!ISNEW())</errorConditionFormula>
        <errorDisplayField>IATSPayment__IATS_Account__c</errorDisplayField>
        <errorMessage>iATS Merchant Account cannot be adjusted.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
