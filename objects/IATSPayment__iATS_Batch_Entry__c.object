    <validationRules>
        <fullName>IATSPayment__Credit_Card_Number</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(IATSPayment__Method_of_Payment__c, &apos;Credit Card&apos;),
IATSPayment__Credit_Card_Number__c == &apos;&apos;
)</errorConditionFormula>
        <errorDisplayField>IATSPayment__Credit_Card_Number__c</errorDisplayField>
        <errorMessage>Credit Card Number must be provided if the method of payment is set to &apos;Credit Card&apos;.</errorMessage>
