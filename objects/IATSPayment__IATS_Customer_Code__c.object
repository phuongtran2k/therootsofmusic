    <validationRules>
        <fullName>IATSPayment__Credit_Card_Number_Format</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 

AND( 
IATSPayment__Complete_Batch_Update__c == true, 
!CONTAINS(&apos;*&apos;,IATSPayment__Credit_Card_Number__c),
!CONTAINS(&apos;X&apos;,IATSPayment__Credit_Card_Number__c),
LEN(IATSPayment__Credit_Card_Number__c)&gt; 4, 
OR(
IATSPayment__Credit_Card_Type__c = &apos;Mastercard&apos;, 
IATSPayment__Credit_Card_Type__c = &apos;MC&apos;),
NOT(REGEX(IATSPayment__Credit_Card_Number__c, &apos;^5[1-5][0-9]{14}$&apos;))), 

AND( 
IATSPayment__Complete_Batch_Update__c == true, 
!CONTAINS(&apos;*&apos;,IATSPayment__Credit_Card_Number__c),
!CONTAINS(&apos;X&apos;,IATSPayment__Credit_Card_Number__c),
LEN(IATSPayment__Credit_Card_Number__c)&gt; 4, 
OR(
IATSPayment__Credit_Card_Type__c = &apos;Visa&apos;, 
IATSPayment__Credit_Card_Type__c = &apos;Visa&apos;),
NOT(REGEX(IATSPayment__Credit_Card_Number__c, &apos;^4[0-9]{12}(?:[0-9]{3})?$&apos;))), 

AND(
IATSPayment__Complete_Batch_Update__c == true, 
!CONTAINS(&apos;*&apos;,IATSPayment__Credit_Card_Number__c),
!CONTAINS(&apos;X&apos;,IATSPayment__Credit_Card_Number__c),
LEN(IATSPayment__Credit_Card_Number__c)&gt; 4, 
OR( 
IATSPayment__Credit_Card_Type__c = &apos;Discover&apos;,
IATSPayment__Credit_Card_Type__c = &apos;DSC&apos;), 
NOT(REGEX(IATSPayment__Credit_Card_Number__c, &apos;^6(?:011|5[0-9]{2})[0-9]{12}$&apos;))), 

AND(
IATSPayment__Complete_Batch_Update__c == true, 
!CONTAINS(&apos;*&apos;,IATSPayment__Credit_Card_Number__c),
!CONTAINS(&apos;X&apos;,IATSPayment__Credit_Card_Number__c),
LEN(IATSPayment__Credit_Card_Number__c)&gt; 4,  
OR(
IATSPayment__Credit_Card_Type__c = &apos;American Express&apos;,
IATSPayment__Credit_Card_Type__c = &apos;AMX&apos;), 
NOT(REGEX(IATSPayment__Credit_Card_Number__c, &apos;^3[47][0-9]{13}$&apos;))) 
)</errorConditionFormula>
