    <validationRules>
        <fullName>IATSPayment__Credit_Card_Number_Format</fullName>
        <active>true</active>
        <errorConditionFormula>OR(

AND(
ISPICKVAL(IATSPayment__Credit_Card_Type__c, &apos;Mastercard&apos;),
IATSPayment__Credit_Card_Number__c != &apos;&apos;,
NOT(REGEX(IATSPayment__Credit_Card_Number__c, &apos;^5[1-5][0-9]{14}$&apos;))),

AND(
ISPICKVAL(IATSPayment__Credit_Card_Type__c, &apos;Visa&apos;),
IATSPayment__Credit_Card_Number__c != &apos;&apos;,
NOT(REGEX(IATSPayment__Credit_Card_Number__c, &apos;^4[0-9]{12}(?:[0-9]{3})?$&apos;))),

AND(
ISPICKVAL(IATSPayment__Credit_Card_Type__c, &apos;Discover&apos;),
IATSPayment__Credit_Card_Number__c != &apos;&apos;,
NOT(REGEX(IATSPayment__Credit_Card_Number__c, &apos;^6(?:011|5[0-9]{2})[0-9]{12}$&apos;))),

AND(
ISPICKVAL(IATSPayment__Credit_Card_Type__c, &apos;American Express&apos;),
IATSPayment__Credit_Card_Number__c != &apos;&apos;,
NOT(REGEX(IATSPayment__Credit_Card_Number__c, &apos;^3[47][0-9]{13}$&apos;)))
)</errorConditionFormula>
        <errorDisplayField>IATSPayment__Credit_Card_Number__c</errorDisplayField>
        <errorMessage>Credit card number is incorrect.</errorMessage>
    </validationRules>
